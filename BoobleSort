    public static void main(String[] args){
        int temp, tacts = 0;
        boolean sorted = false;
        while (!sorted){
            sorted = true;
            for (int x = 1; x < ar.length; x++) {
                if(ar[x - 1] < ar[x]){
                    temp = ar[x - 1];
                    ar[x - 1] = ar[x];
                    ar[x] = temp;
                    sorted = false;
                    tacts++;
                }
            }
        }
        System.out.println(Arrays.toString(ar));
        System.out.println(tacts);

    }
